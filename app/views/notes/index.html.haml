%h1= t('.title')

%table
  %thead
    %tr
      %th= Note.human_attribute_name :user_id
      %th= Note.human_attribute_name :account_id
      %th= Note.human_attribute_name :content
      %th{:colspan => 3}= t 'actions'
  
  %tbody
    - for note in @notes
      %tr{ :class => cycle(:odd, :even) }
        %td= note.user_id
        %td= note.account_id
        %td= note.content
