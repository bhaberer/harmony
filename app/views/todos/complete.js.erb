$('<%= "#todo_#{@todo.id}" %>').fadeOut('slow', function() {
  $('<%= "#todo_#{@todo.id}" %>').remove();

  $('<%= escape_javascript(render :partial => 'todos/todo', :locals => {:todo => @todo, :hide => true} ) %>').prependTo('<%= @todo.done? ? "#finished" : "#theirs" %>');
  $('<%= "#todo_#{@todo.id}" %>').fadeIn('slow', function() {
     ajaxLinks();
   });
});
